<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>
    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>

    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <protocol>http</protocol>
    
    <!-- title of the document -->
    <documentTitle>Test Progress Report</documentTitle>
        
    <!-- report name - displayed at top-nav -->
    <reportName>NASCA Test Progress Report - </reportName>
        
    <!-- report headline - displayed at top-nav, after reportHeadline -->
    <reportHeadline>This report will show you the status of automated tests for NASCA</reportHeadline>

    <!-- global date format override -->
    <!-- defaults to yyyy-MM-dd -->
    <dateFormat>dd-MM-yyyy</dateFormat>

    <!-- global time format override -->
    <!-- defaults to HH:mm:ss -->
    <timeFormat>HH:mm:ss</timeFormat>
      
    <!-- custom javascript -->
    <scripts>
      <![CDATA[
      $(document).ready(function() {
$(".theme-selector").click();
$("#dashboard-view").removeClass("hide");
]
      });
      ]]>
    </scripts>
        
    <!-- custom styles -->
    <styles>
      <![CDATA[
                
      ]]>
    </styles>
  </configuration>
</extentreports>